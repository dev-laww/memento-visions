; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[MonoCustomResourceRegistry]

ClassPrefix=""
SearchType=0
ResourceScriptDirectories=["res://"]

[application]

config/name="Memento Visions"
run/main_scene="res://Scenes/Main.tscn"
config/features=PackedStringArray("4.3", "C#", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

SceneManager="*res://Scripts/Globals/SceneManager.cs"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"
InteractionManager="*res://Scripts/Globals/InteractionManager.cs"
CraftingManager="*res://Scripts/Globals/RecipeManager.cs"

[dialogue_manager]

general/balloon_path="res://Scenes/UI/Dialogue/Balloon.tscn"
general/uses_dotnet=true
general/states=["SceneManager", "InteractionManager", "Bar"]

[display]

window/size/viewport_width=640
window/size/viewport_height=360
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[dotnet]

project/assembly_name="MementoVisions"

[editor]

naming/scene_name_casing=1

[editor_plugins]

enabled=PackedStringArray("res://addons/AS2P/plugin.cfg", "res://addons/center_pivot_offset/plugin.cfg", "res://addons/dialogue_manager/plugin.cfg", "res://addons/node_library/plugin.cfg", "res://addons/palette_importer/plugin.cfg")

[file_customization]

folder_colors={
"res://Scenes/": "blue",
"res://Scripts/": "pink",
"res://addons/": "gray",
"res://assets/": "orange",
"res://data/": "yellow",
"res://resources/": "teal",
"res://script_templates/": "gray"
}

